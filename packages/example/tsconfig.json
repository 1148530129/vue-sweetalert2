{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es2017",
      "es7",
      "es6",
      "dom"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "outDir": "dist",
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "strictNullChecks": true,
    "removeComments": true,
    "suppressImplicitAnyIndexErrors": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "sourceMap": true,
    "types": [
      "vue-sweetalert2"
    ],
    "rootDir": "./src"
  },
  "include": [
    "./src/*.vue",
    "./src/*.ts",
    "./src/*.js"
  ]
}